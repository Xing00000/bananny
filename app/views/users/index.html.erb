
<div class="container-full">
	<div class="bg">
		<div class="container">
			<div id="search-result">
				<% if @search_nanny %>

					<div class="row">
				  	<%= render :partial => "result" ,:collection => @search_nanny, :as => :nanny %>
					</div>
				<%end%>
			</div>

			<div id="user-booking">
				<% if current_user && current_user.registered? == nil%>
				  <%= link_to "保母註冊",new_user_nanny_path(current_user),:class => "btn btn-primary"%>
				  <%= link_to "爸媽註冊",new_user_parent_path(current_user),:class => "btn btn-primary"%>
				<% end %>
			</div>
		</div>
	</div>
</div>


